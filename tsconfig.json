{
    "compilerOptions": {
      "module": "ESNext",
      "moduleResolution": "node",
      "target": "ES2020",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "outDir": "./dist",
      "sourceMap": true,
      "allowImportingTsExtensions": true,
      "noEmit":true
    },
    "include": ["src"]
  }
  